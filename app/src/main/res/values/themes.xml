<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.CastToTv" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/cr_white</item>
        <item name="colorPrimaryVariant">@color/cr_white</item>
        <item name="colorOnPrimary">@color/cr_black</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/cr_dodger_blue_light_1</item>
        <item name="colorSecondaryVariant">@color/cr_dodger_blue_dark</item>
        <item name="colorOnSecondary">@color/cr_black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
        <item name="attr_lblack_dwhite_80">@color/cr_black_80</item>
        <item name="attr_lblack_dwhite_70_70">@color/cr_black_70</item>
        <item name="attr_lblack_dwhite_12_12">@color/cr_black_12</item>
        <item name="attr_lblack_dwhite_80_80">@color/cr_black_80</item>
        <item name="attr_subheading">@color/cr_black_60</item>
        <item name="attr_black_white_20_20">@color/cr_black_20</item>
        <item name="attr_white_orange">@color/cr_pumpkin</item>
        <item name="attr_lblack_dwhite_60_60">@color/cr_black_60</item>
        <item name="l_zumthor_b_black_pearl_1">@color/cr_zumthor</item>
        <item name="l_white_b_black">@color/cr_white</item>
        <item name="l_black50_b_strome">@color/cr_black_50</item>
        <item name="l_emperor_b_silver_chalice">@color/cr_emperor</item>
        <item name="l_white_b_ebony_clay">@color/cr_white</item>
        <item name="l_white_b_firefly">@color/cr_white</item>


    </style>

    <style name="Theme.CastToTv.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.CastToTv.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.CastToTv.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />


</resources>